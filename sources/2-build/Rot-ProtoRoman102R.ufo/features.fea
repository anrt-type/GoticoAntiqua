feature liga {
sub c t by c_t;
sub f f by f_f;
sub f l by f_l;
sub longs i by longs_i;
sub longs longs by longs_longs;
sub longs t by longs_t;
} liga;

feature hlig {
sub c t by c_t;
sub f f by f_f;
sub f l by f_l;
sub longs i by longs_i;
sub longs longs by longs_longs;
sub longs t by longs_t;
} hlig;

feature hist {
sub Q by Q.ss01;
sub r by r.ss01;
sub s by longs;
sub @AllLetters longs' [emspace enspace threeperemspace fourperemspace space thinspace hairspace period] by s;
} hist;

feature salt {
sub Q by Q.ss01;
sub r by r.ss01;
sub s by longs;
sub @AllLetters longs' [emspace enspace threeperemspace fourperemspace space thinspace hairspace period] by s;
} salt;

feature ss01 {
sub Q by Q.ss01;
} ss01;

feature ss02 {
sub r by r.ss01;
} ss02;

feature ss03 {
sub s by longs;
sub @AllLetters longs' [emspace enspace threeperemspace fourperemspace space thinspace hairspace period] by s;
} ss03;

